{
  "name": "quick-url-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint . --ext .ts --fix",
    "start:urls": "npm -w apps/urls run start",
    "start:users": "npm -w apps/users run start",
    "build": "npm -w apps/users run build && npm -w apps/urls run build",
    "build:users": "npm -w apps/users run build",
    "build:urls": "npm -w apps/urls run build",
    "dev:users": "npm -w apps/users run start:dev",
    "dev:urls": "npm -w apps/urls run start:dev",
    "dev:all": "npm run dev:users & npm run dev:urls",
    "test": "npm -w apps/users run test && npm -w apps/urls run test",
    "test:coverage": "npm -w apps/users run test:coverage && npm -w apps/urls run test:coverage",
    "prisma:generate:users": "npx prisma generate --schema=./apps/users/prisma/schema.prisma",
    "prisma:generate:urls": "npx prisma generate --schema=./apps/urls/prisma/schema.prisma",
    "prisma:generate:all": "npm run prisma:generate:users && npm run prisma:generate:urls",
    "prisma:migrate:dev:users": "npx prisma migrate dev --schema=./apps/users/prisma/schema.prisma",
    "prisma:migrate:dev:urls": "npx prisma migrate dev --schema=./apps/urls/prisma/schema.prisma",
    "prisma:migrate:dev:all": "npm run prisma:migrate:dev:users && npm run prisma:migrate:dev:urls",
    "prisma:migrate:deploy:users": "npx prisma migrate deploy --schema=./apps/users/prisma/schema.prisma",
    "prisma:migrate:deploy:urls": "npx prisma migrate deploy --schema=./apps/urls/prisma/schema.prisma",
    "prisma:migrate:deploy:all": "npm run prisma:migrate:deploy:users && npm run prisma:migrate:deploy:urls"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "9.31.0",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.5.1",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "prettier": "3.6.2",
    "typescript-eslint": "8.37.0",
    "@types/node": "24.0.14",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-v8": "3.2.4",
    "prisma": "6.12.0",
    "supertest": "7.1.3",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsup": "8.5.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  }
}
